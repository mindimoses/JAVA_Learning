package Moses;

public class Lesson1 {

	// Logos
		   public static void main(String args[])
		   {
		      int a=1;
		  
		      switch(a)
		      {
		         case 0:
		             System.out.println("Case Zero");
		             break;
		         case 1:
		             System.out.println("Case One");
		             break;
		         default:
		             System.out.println("Default Case");
		       }
		    }
		 }


